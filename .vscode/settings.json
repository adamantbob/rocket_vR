{
    "rust-analyzer.cargo.features": [
        "rp2350"
    ],
    "rust-analyzer.cargo.target": "thumbv8m.main-none-eabihf",
    "rust-analyzer.cargo.noDefaultFeatures": false,
    "rust-analyzer.cargo.allTargets": false,
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "check",
        "--workspace",
        "--exclude",
        "rocket-sitl-ksp",
